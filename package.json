{
  "name": "wedplan",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:deploy": "vue-tsc --noEmit && VITE_BASE_PATH=/wedplan/ vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.ts,.js,.tsx",
    "lint:fix": "eslint . --ext .vue,.ts,.js,.tsx --fix",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@vueuse/gesture": "^2.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "papaparse": "^5.5.3",
    "pinia": "^3.0.4",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/papaparse": "^5.5.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "happy-dom": "^15.11.7",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^2.1.8",
    "vue-tsc": "^3.1.4"
  }
}
